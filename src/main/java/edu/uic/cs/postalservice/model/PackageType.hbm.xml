<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="edu.uic.cs.postalservice.model.PackageType" table="PACKAGE_TYPE">
        <meta attribute="class-description">
            This class contains the Package Type information.
        </meta>
        <id name="package_id" column="PACKAGE_ID">
            <generator class="native"/>
        </id>
        <property name="package_name" column="PROPERTY_NAME"/>

    </class>



</hibernate-mapping>